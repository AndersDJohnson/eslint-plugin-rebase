{
  "name": "eslint-plugin-rebase",
  "version": "0.1.0",
  "description": "Start ESLinting new code, without fixing all the old.",
  "main": "index.js",
  "bin": {
    "eslint-rebase": "dist/cli.js"
  },
  "files": [
    "dist",
    "!__tests__",
    "docs",
    "!.*"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build:publish": "npm run build && cp -r ../../{docs,README.md,LICENSE}",
    "build:watch": "tsc -w",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "Anders D. Johnson",
  "repository": "AndersDJohnson/eslint-plugin-rebase",
  "license": "MIT",
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.7.0",
    "@types/eslint": "^6.1.8",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "eslint": "^7.30.0",
    "jest": "^25.1.0",
    "strip-indent": "^3.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "eslint": ">=6 <8"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "lodash": "^4.17.15",
    "yargs": "^15.3.0"
  }
}
